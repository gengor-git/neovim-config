	"Footnote": {
		"prefix": "ftn",
		"body": "[^$1] $0\n\n[^$1]: $2",
		"description": "Footnote entry"
	},

	"Image": {
		"prefix": "img",
		"body": "![$1]($2)${3:{width=${4:100%}}}$0",
		"description": "Insert an image."
	},

	"Weblink": {
		"prefix": "link",
		"body": "[$1]($2) $0",
		"description": "Link to an URL"
	},

	"Definition": {
		"prefix": "def",
		"body": "$1\n\n: $2\n\n$0",
		"description": "Simple definition."
	},

	"Asian Support": {
		"prefix": "asia",
		"body": "CJKmainfont: \"${1:Arial Unicode MS}\"",
		"description": "Use Unicode font for Asian characters."
	},
	"Font Awesome": {
		"prefix": "fa",
		"body": "\\fa$1{} $0",
		"description": "Enter Latex font awesome icon."
	},
	"Inline PlantUML": {
		"prefix": "puml",
		"body": "```{.plantuml caption=\"$1\"}$0\n```",
		"description": "Use inline PlantUML in Markdown codeblocks."
	},
	"Quote Fix": {
		"prefix": "quotes",
		"body": "\\setmonofont[Mapping=tex-ansi]{Courier New}",
		"description": "Fix for settings correct quotes."
	},
	"Latex Colors": {
		"prefix": "lcolors",
		"body": "\\definecolor{green}{HTML}{91c32c}\n\\definecolor{arvato-blue1}{HTML}{0076b2}\n\\definecolor{arvato-blue2}{HTML}{1d577e}\n\\definecolor{arvato-greydark}{HTML}{646464}\n\\definecolor{arvato-greylight}{HTML}{444444}\n\\definecolor{arvato-red}{HTML}{e90f40}\n\\definecolor{arvato-orange}{HTML}{f05d25}\n\\definecolor{arvato-yellow}{HTML}{ffc105}\n\\definecolor{arvato-green}{HTML}{91c32c}\n\\definecolor{arvato-blue3}{HTML}{0076b2}\n\\definecolor{arvato-blue4}{HTML}{1983ba}\n\\definecolor{arvato-blue5}{HTML}{3391c1}\n\\definecolor{arvato-blue6}{HTML}{4c9fc9}\n\\definecolor{arvato-blue7}{HTML}{66add1}\n\\definecolor{arvato-blue8}{HTML}{7fbad8}\n\\definecolor{arvato-blue9}{HTML}{99c8e0}\n\\definecolor{arvato-blue10}{HTML}{b2d6e8}\n\\definecolor{arvato-blue11}{HTML}{cce4f0}\n\\definecolor{arvato-blue12}{HTML}{d9ebf4}\n\\definecolor{arvato-blue13}{HTML}{e5f1f7}\n\\definecolor{arvato-blue14}{HTML}{EDF5FA}",
		"description": "CI colors for Latex"
	},
	"Color Text": {
		"prefix": "tcolor",
		"body": "\\textcolor{${0:arvato-blue1}}{$1}"
	}
