{
	"Document head white": {
		"prefix": "headw",
		"body": [
			"---",
			"title: \"$1\"",
			"subtitle: \"$2\"",
			"author: [$3]",
			"date: $4",
			"version: \"V1.0\"",
			"header-center: \"V1.0\"",
			"classification: \"${5:Internal}\"",
		  "footer-center: \"${5:Internal}\"",
			"tags: [$6]",
			"titlepage: true",
			"titlepage-color: FFFFFF",
			"titlepage-text-color: 1D577E",
			"titlepage-rule-color: 1D577E",
			"titlepage-rule-height: 1",
			"lang: ${7:en}",
			"toc: true",
			"lot: true",
			"lof: true",
			"logo: $8",
			"titlepage-logo: $9",
			"toc-own-page: ${10:true}",
			"...",
			"$0"
		],
		"description": "Standard header with white background and darker blue line."
	},

	"Document head standard white and new blue": {
		"prefix": "headstd",
		"body": [
